{
  "name": "reactnativereduxstarterkit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:web": "",
    "build:ios": "",
    "build:android": "",
    "build:chrome": "",
    "dev:ios": "react-native run-ios",
    "dev:android": "react-native run-android",
    "dev:web": "webpack-dev-server --port 3000 --config web/webpack.config.dev.js --inline --hot --colors --quiet",
    "start": "yarn dev:ios",
    "storybook:ios": "storybook start -p 7007",
    "storybook:web": "start-storybook -p 9001 -c web/.storybook",
    "test": "yarn test:jest",
    "test:storybook": "storyshots -c web/.storybook --webpack",
    "test:jest": "jest src/",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 6006"
  },
  "babel": {
    "presets": [
      "react-native-stage-0"
    ],
    "plugins": [
      "transform-regenerator"
    ]
  },
  "jest": {
    "preset": "jest-react-native",
    "setupFiles": [
      "./test_config/setup.js"
    ],
    "globals": {
      "__DEV__": true,
      "__TEST__": true
    },
    "moduleNameMapper": {
      "@kadira/react-native-storybook": "@kadira/storybook"
    }
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-react"
    ],
    "plugins": [
      "jest"
    ],
    "env": {
      "jest/globals": true
    }
  },
  "dependencies": {
    "@kadira/react-native-storybook": "^2.3.0",
    "@kadira/storyshots": "^2.1.0",
    "axios": "^0.16.0",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-polyfill": "^6.23.0",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "^0.0.5-beta.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "react": "^15.4.0",
    "react-native": "^0.41.0",
    "react-native-cli": "^2.0.1",
    "react-native-elements": "^0.10.3",
    "react-native-vector-icons": "^4.0.1",
    "recompose": "^0.22.0",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-cli": "^1.9.0",
    "redux-saga": "^0.14.4",
    "storyshots": "^3.2.2"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "19.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react-native": "1.9.1",
    "babel-preset-react-native-stage-0": "^1.0.0",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.1.1",
    "git-hooks": "^1.1.8",
    "jest": "19.0.2",
    "jest-react-native": "^18.0.0",
    "react-dom": "^15.4.2",
    "react-hot-loader": "2.0.0-alpha-4",
    "react-native-mock": "^0.3.1",
    "react-native-web": "0.0.70",
    "react-navigation": "^1.0.0-beta.7",
    "react-redux": "^5.0.3",
    "react-test-renderer": "~15.4.1",
    "redux-devtools-extension": "^2.13.0",
    "remote-redux-devtools": "^0.5.7",
    "shelljs": "^0.7.7",
    "string": "^3.3.3",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.14.1"
  }
}
